{
  "name": "PureTickets",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": "broccolai",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "$npm_execpath run lint && $npm_execpath run build && node ./dist/server.js",
    "lint": "eslint \"src/**\" --fix",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "module-alias": "^2.2.2",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "18.11.9",
    "@types/node-fetch": "2.6.2",
    "@types/sqlite3": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "4.8.4"
  },
  "_moduleAliases": {
    "@api": "dist/api",
    "@lib": "dist/lib",
    "@constructs": "dist/constructs"
  }
}
